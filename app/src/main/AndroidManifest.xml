<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.mobile.teamc2tech.ktchannel">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_kl_tv"
        android:label="@string/app_name"
        android:roundIcon="@drawable/ic_kl_tv"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity android:name="com.mobile.teamc2tech.ktchannel.SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name="com.mobile.teamc2tech.ktchannel.FullScreenVideoActivity"
            android:configChanges="keyboardHidden|orientation|screenSize" />


    </application>

</manifest>